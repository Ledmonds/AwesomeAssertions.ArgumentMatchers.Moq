<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45;netstandard2.0</TargetFrameworks>
    <Version>0.1.1</Version>
    <PackageId>Moq.ArgumentMatchers.FluentAssertions</PackageId>
    <PackageTags>netstandard;Moq;FluentAssertions</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Ronald Bosma</Authors>
    <Description>Contains Moq argument matchers using FluentAssertions</Description>
    <RepositoryUrl>https://github.com/ronaldbosma/Moq.ArgumentMatchers.FluentAssertions</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Moq.ArgumentMatchers.FluentAssertions.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FluentAssertions" Version="[5.1.1,6)" />
    <PackageReference Include="Moq" Version="[4.7,5)" />
  </ItemGroup>

</Project>
